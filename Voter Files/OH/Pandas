#this bit imports pandas and other data processing packages that makes this project easier
import pandas as pd
import datetime
from numpy import random
import matplotlib.pyplot as plt
import sys #only needed to determine Python version number
import matplotlib #only needed to determine Matplotlib version number and to use for data analysis, super fun

#creates two dataframes from csv files 
Location = r'C:\Users\Tyler\Documents\Data Projects\National Voter Data File\Ohio\Voter Files\July30.txt'
df_a = pd.read_csv(Location, usecols=['SOS_VOTERID', 'LAST_NAME', 'FIRST_NAME', 'REGISTRATION_DATE', 'DATE_OF_BIRTH', 'VOTER_STATUS', 'PARTY_AFFILIATION'])
Location2 = r'C:\Users\Tyler\Documents\Data Projects\National Voter Data File\Ohio\Voter Files\August6.txt'
df_b = pd.read_csv(Location2, usecols=['SOS_VOTERID', 'LAST_NAME', 'FIRST_NAME', 'REGISTRATION_DATE', 'DATE_OF_BIRTH', 'VOTER_STATUS', 'PARTY_AFFILIATION'])

merged = pd.merge(df_a, df_b, on='SOS_VOTERID', how='outer', suffixes=('_week1', '_week2'))


test = merged.loc[pd.isnull(merged.LAST_NAME_week2)]
test = test.loc[pd.isnull(test.FIRST_NAME_week2)]
test1 = merged.loc[pd.isnull(merged.LAST_NAME_week1)]
test1 = test1.loc[pd.isnull(test1.FIRST_NAME_week1)]

test.to_csv('Purged.csv')
test1.to_csv('NewReg.csv')
